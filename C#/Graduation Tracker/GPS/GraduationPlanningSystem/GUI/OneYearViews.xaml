<UserControl x:Class="GraduationPlanningSystem.OneYearViews"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:gui="clr-namespace:GraduationPlanningSystem"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="450">
    
        <Expander>
        <Expander.Header>
            <TextBlock Name="txtHeader" Text="Year" Foreground="Black" FontWeight="Bold" HorizontalAlignment="Center"/>
        </Expander.Header>
            <Grid x:Name="gridMain">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="34*"/>
                    <ColumnDefinition Width="33*"/>
                    <ColumnDefinition Width="33*"/>
                </Grid.ColumnDefinitions>
                <!--<gui:SemesterControl VerticalAlignment="Top"  Grid.Column="0"/>
            <gui:SemesterControl VerticalAlignment="Top"  Grid.Column="1"/>
            <gui:SemesterControl VerticalAlignment="Top"  Grid.Column="2"/>-->
            </Grid>
    </Expander>
   
</UserControl>
